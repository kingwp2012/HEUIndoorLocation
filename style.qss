    background:transparent;
}
QPushButton#nextPage:enabled:pressed
{
    image:url(:/new/White/nextpage3);
    background:transparent;
}
QPushButton#prePage:enabled
{
    image:url(:/new/White/prepage1);
    background:transparent;
}
QPushButton#prePage:enabled:hover
{
    image:url(:/new/White/prepage2);
    background:transparent;
}
QPushButton#prePage:enabled:pressed
{
    image:url(:/new/White/prepage3);
    background:transparent;
}
QPushButton#firstPage:enabled
{
    image:url(:/new/White/firstpage1);
    background:transparent;
}
QPushButton#firstPage:enabled:hover
{
    image:url(:/new/White/firstpage2);
    background:transparent;
}
QPushButton#firstPage:enabled:pressed
{
    image:url(:/new/White/firstpage3);
    background:transparent;
}
QPushButton#lastPage:enabled
{
    image:url(:/new/White/lastpage1);
    background:transparent;
}
QPushButton#lastPage:enabled:hover
{
    image:url(:/new/White/lastpage2);
    background:transparent;
