
/*************基站表单***************/


BaseStationForm
{
    border:1px solid red;
}

BaseStationForm QLabel
{
  font-weight:bold;

}
QScrollArea#scrollArea
{
    background:white;
    border-left:1px solid black;
    border-right:1px solid black;
    border-bottom:none;
    border-top:none;
}
QWidget#widget_1
{
    border-left:none;
    border-right:none;
    border-top:none;
    background:white;
}

QWidget#widget_2
{
    background:transparent;
    width:500;
    height:200;
    border-top:1px solid rgb(111,156,207);
    border-left:1px solid rgb(111,156,207);
    border-right:1px solid rgb(111,156,207);
    border-bottom:1px solid rgb(111,156,207);
}
QLineEdit
{
    width:300;
    height:30;
    border-radius:4px;
    border:1px solid rgb(111,156,207);
    background:white;
}
QLineEdit:enabled
{
    color:rgb(84,84,84);
}
QLineEdit:enabled:hover,QLineEdit:enabled:focus
{
    color:rgb(51,51,51);
}
QLineEdit:!enabled
{
    color:rgb(80,80,80);
}
QLabel
{
    color:black;
    font-size:16px;
}

QPushButton#chk_btn
{
    background:#4876FF;
    color:white;
}
QPushButton#chk_btn:hover
{
    background:#87CEFF;
}
QPushButton#chk_btn:pressed
{
    background:#436EEE;
}
QPushButton#can_btn
{
    background:#EE5C42;
    color:white;
}
QPushButton#can_btn:hover
{
    background:#EE8262;
}
QPushButton#can_btn:pressed
{
    background:#EE4000;
}

/****************基站管理表格*******************/

QTabWidget::pane {
        border: none;
        border-top: 3px solid rgb(0, 78, 161);
        background: rgb(187, 212, 238);
}
QTabWidget::tab-bar {
        border: none;
}
QTabBar::tab {
        border: none;
        border-top-left-radius: 4px;
        border-top-right-radius: 4px;
        color: white;
        background: rgb(120, 170, 220);
        height: 28px;
        min-width: 85px;
        margin-right: 5px;
        padding-left: 5px;
        padding-right: 5px;
}
QTabBar::tab:hover {
        background: rgb(0, 78, 161);
}
QTabBar::tab:selected {
        color: white;
        background: rgb(0, 78, 161);
}

QTabWidget#tabWidget::pane {
        border: 1px solid rgb(111, 156, 207);
        background: rgb(232, 241, 252);
        margin-top: -1px;
}

QTabBar#tabBar::tab {
        border: 1px solid rgb(111, 156, 207);
        border-bottom: none;
        color: rgb(70, 71, 73);
        background: transparent;
}
QTabBar#tabBar::tab:hover {
        color: rgb(2, 65, 132);
}
QTabBar#tabBar::tab:selected {
        color: rgb(2, 65, 132);
        background: rgb(232, 241, 252);
}

QPushButton
{
    border-radius:4px;
    border:none;
    width:80px;
    height:30px;
}

JzWidget QPushButton:enabled
{
    background:rgb(120,170,220);
    color:white;
}
JzWidget QPushButton:!enabled
{
    background:rgb(180,180,180);
}
JzWidget QPushButton:enabled:hover
{
    background:rgb(100,160,220);
}
JzWidget QPushButton:enabled:pressed
{
    background:rgb(0,78,161);
}
QPushButton#nextPage:enabled
{
